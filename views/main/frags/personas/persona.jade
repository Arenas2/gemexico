md-card
	md-input-container(form="persona")
		label Nombre
		input(ng-model="persona.nombre")
	md-input-container
		label Edad
		input(ng-model="persona.edad")
	md-input-container
		label imagenId
		input(ng-model="persona.imagenID")
	md-button(ng-click="nuevaPersona();")
md-card
	md-card-content
		h1 {{personas[id].Nombre}}
md-divider
md-card
	md-card-content
		|	Edad: {{personas[id].Edad}}
